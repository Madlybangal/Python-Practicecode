#Multiplication table function using for loop

import pandas as pd
list_table=[2,3,4,5,6,7,8,10]

def mult_table(list_table):
    table_dic={}
    for num in list_table:
        table1=[]
        for i in list(range(1,11)):
            table1.append(num*i)
        table_dic[num]=table1
    return pd.DataFrame(table_dic)

print(mult_table([4,5]))
