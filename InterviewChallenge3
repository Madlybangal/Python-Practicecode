#majority element find from list

def findMajority(arr,n):
    maxCount=0
    index=-1
    for i in range(n):
        count=0
        for j in range(n):
            if(arr[i]==arr[j]):
                count+=1
        #update maxCount if count of current element is greater
        if(count>maxCount):
            maxCount=count
            index=i
    if(maxCount>n//2):
        print(arr[index])
    else:
        print("No Majority element Found")     


nums=[3,3,4,5,3,2,4,3,3,4,3,3]
n=len(nums)
findMajority(nums,n)


